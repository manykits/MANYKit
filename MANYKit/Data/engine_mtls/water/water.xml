					<var index="7" name="LightDiffuse_Dir" nr="1"/>
					<var index="8" name="LightSpecular_Dir" nr="1"/>
				</c>
				<s>
					<var index="0" name="SampleDuDv" st="sampler2D" sf="LINEAR_LINEAR" sc_0="REPEAT" sc_1="REPEAT"/>
