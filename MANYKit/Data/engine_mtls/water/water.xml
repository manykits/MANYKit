					<var type="glsl" file="water_glsl_ps.fx"/>
				</p>
			</shader>
			<renderproperty>
				<Alpha>
					<p param="BlendEnabled" value="true"/>
					<p param="SrcBlend" value="SBM_SRC_ALPHA"/>
