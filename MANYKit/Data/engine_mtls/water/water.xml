				</Alpha>
				<Depth>
					<p param="Enabled" value="true"/>
					<p param="Writable" value="false"/>
					<p param="Compare" value="CM_LEQUAL"/>
				</Depth>
				<Cull>
					<p param="Enabled" value="true"/>
					<p param="CCWOrder" value="true"/>
				</Cull>
				<Wire>
					<p param="Enabled" value="false"/>
				</Wire>
			</renderproperty>
		</pass>
